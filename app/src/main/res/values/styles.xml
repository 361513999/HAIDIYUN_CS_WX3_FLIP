     <style name="ThemeActivity" mce_bogus="1">
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- activity动画 -->
    <style name="AnimationActivity" mce_bogus="1">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:activityOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_right_out</item>
        <item name="android:taskOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:taskOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:taskCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:taskCloseExitAnimation">@anim/push_right_out</item>
        <item name="android:taskToFrontEnterAnimation">@anim/push_left_in</item>
        <item name="android:taskToFrontExitAnimation">@anim/push_left_out</item>
        <item name="android:taskToBackEnterAnimation">@anim/push_right_in</item>
        <item name="android:taskToBackExitAnimation">@anim/push_right_out</item>
    </style>
    
       <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">8dp</item>
         <item name="android:paddingRight">8dp</item>
          <item name="android:paddingBottom">4dp</item>
           <item name="android:paddingTop">4dp</item>
    </style>
    
      <style name="normal_text">
        <item name="android:shadowColor">#000011</item>
        <item name="android:shadowDx">4.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
     <style name="scrollbar_bitmap_style">
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:fadeScrollbars">false</item>
        <item name="android:scrollbarThumbVertical">@drawable/home_lv_scrollbar</item>
        <item name="android:scrollbarThumbHorizontal">@drawable/home_lv_scrollbar</item>
        <item name="android:scrollbarSize">5dp</item>
    </style>
